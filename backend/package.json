  "dependencies": {
    "apollo-server": "^3.13.0",
    "argon2": "^0.31.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "ts-node-dev": "^2.0.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
